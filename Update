-- StarterGui.Reave.MainFrame.Profile.HeadImage.ImageLabel.SetImage
local function C_d()
local script = G2L["d"];
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local userId = player.UserId
	
	local imageLabel = script.Parent
	
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	imageLabel.Image = content
	
end;
task.spawn(C_d);
-- StarterGui.Reave.MainFrame.Profile.DisplayName.SetDisplay
local function C_f()
local script = G2L["f"];
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local playerName = player.DisplayName
	
	local userLabel= script.Parent
	
	userLabel.Text = playerName
end;
task.spawn(C_f);
-- StarterGui.Reave.MainFrame.Profile.Username.SetUser
local function C_11()
local script = G2L["11"];
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	local playerName = player.Name
	
	local userLabel= script.Parent
	
	userLabel.Text = "@" .. playerName
end;
task.spawn(C_11);
-- StarterGui.Reave.MainFrame.Close.CloseScript
local function C_15()
local script = G2L["15"];
	local gui = script.Parent.Parent
	local button = script.Parent
	
	button.MouseButton1Click:Connect(function()
		gui.Visible = false
	end)
end;
task.spawn(C_15);

return G2L["1"], require;
